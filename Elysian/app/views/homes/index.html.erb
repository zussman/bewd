<h2 class="page-header">Homes</h2>
<%= link_to "Add Home", new_home_path %>
<div class="row">
  <div class="col-sm-12">
    <table class="table table-striped table-condensed display" id="homes">
      <thead>
        <tr>
          <th>Account Number</th>
          <th>Status</th>
          <th>Utility</th>
          <th>Address</th>
          <th>Sq. Ft.</th>
          <th>Own</th>
          <th>Apt.</th>
          <th>Owner</th>
        </tr>
      </thead>
      <tbody>
        <% @homes.each do |home| %>
          <tr>
            <td><%= link_to home.account_number, home_path(home) %></td>
            <td>Status</td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
